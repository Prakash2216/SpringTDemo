<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="student"	class="com.tutorial.spring.eventhandling.Student">
	<property name="name"	value="Sumit"/>
	<property name="age"	value="29"/>
</bean>
<bean id="cstartEventHandler"	class="com.tutorial.spring.eventhandling.CStartedEventHandler"/>
<bean id="sstopEventHandler"	class="com.tutorial.spring.eventhandling.SStopEventHandler"></bean>
</beans>